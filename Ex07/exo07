void ft_is_negative(int n)
{
    if (n < 0)
    {
        Console.WriteLine("N\n");
    }
    else
    {
        Console.WriteLine("P\n"); // Affiche 'P' si n est positif ou nul
    }
}

int main()
{
    ft_is_negative(-5); // Affichera 'N'
    ft_is_negative(0); // Affichera 'P'
    ft_is_negative(10); // Affichera 'P'

    return 0;
}
